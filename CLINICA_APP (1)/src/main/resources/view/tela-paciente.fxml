<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TelaPaciente">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="15.0">
         <Label style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Área do Paciente">
            <VBox.margin>
               <Insets />
            </VBox.margin></Label>
         <Button fx:id="btnAgendarConsultas" text="Agendar Consultas" />
         <Button fx:id="btnMinhasConsultas" text="Minhas Consultas" />
      </VBox>
   </children>
</AnchorPane>
